<style>
         #{{ section.id }} .main-footer{
         width:100%;
         background-color:{{section.settings.footer-background}};
         }
         #{{ section.id }} .inner-footer{
         width:100%;
         }
         #{{ section.id }} .logo-footer{
         padding:15px 17px 5px 17px;
         display: flex;
         justify-content: center;
         }
         #{{ section.id }} .logo-img{
         width:10%;
         } 
         #{{ section.id }} .logo-img img{
         width:100%;
         }
         #{{ section.id }} .policy-data{
         padding:5px 337px 10px 337px;  
         }
         #{{ section.id }} .main-ancher{
         text-align: center;
         display: flex;
         justify-content: center; 
         margin:0;
         }
         #{{ section.id }} .main-ancher a{
         font-size:{{section.settings.ancher-bttn-font}}px;
         color:{{section.settings.bttn-color}};
         text-decoration: none;
         font-weight:400;
         
         } 
         #{{ section.id }} .main-ancher a:hover{
         text-decoration: underline;
         text-underline-offset:6px;
         } 
         #{{ section.id }} .footer-discription{
         font-size:{{section.settings.footr-desript-font}}px;
         color:{{section.settings.footer-discript-colors}};
         font-weight:400;
         text-align: center;
         margin:0;
         }
         #{{ section.id }} .Contact-data{
         padding:10px 17px;
         }
         #{{ section.id }} .contact-text{
         margin:0;  
         line-height:21px;
         font-weight:700; 
         text-align:center;
         }
         #{{ section.id }} .number-ancher{
         text-align: center;
         margin:0;
         }
         #{{ section.id }} .number-ancher a{
         text-decoration:none;
         line-height:20px;
         font-weight:700;
         
         }
         #{{ section.id }} .number-ancher a:hover{
         text-decoration:underline;
         }
         #{{ section.id }} .neuro-md-email{
          
         line-height:20px;
         font-weight:700;
         
         margin:0;
         text-align: center;
         }
         .timing{
         margin:0;
         
         line-height:20px;
         text-align: center;
         font-weight:400;
         
         }
         #{{ section.id }} .footer-img{
         padding:5px 17px 3px 17px;
         display: flex;
         justify-content:center;
         }
         #{{ section.id }} .footer-image{
         width:7%;
         }
         #{{ section.id }} .footer-img img{
         width:100%;
         }
         #{{ section.id }} .media-rate-para{
         padding:10px 26px;
         text-align: center;
         
         }
         @media only screen and (max-width:767px) {
         #{{ section.id }} .logo-img {
         width: 60%;
         }
         #{{ section.id }} .footer-image {
         width: 30%;
         }
         #{{ section.id }} .footer-image{
         width:13%;
         }
         }
         @media only screen and (max-width:991px) {
         #{{ section.id }} .policy-data{
         padding:5px 20px 10px 20px;  
         } 
         #{{ section.id }} .logo-img {
         width:30%;
         }
         }
      </style>
      <div id="{{ section.id }}">
      <div class="main-footer">
         <div class="inner-footer">
            <div class="logo-footer">
               <div class="logo-img">
                  {% if section.settings.footer-img  != blank %}
             <a href="/"> <img src="{{ section.settings.footer-img | img_url: 'large' }}" alt="{{ section.settings.footer-img}}"></a>
            {% endif %}
               </div>
            </div>
            <div class="policy-data">
               <p class="main-ancher">
                  <a href="" class="Terms-ancher" style="padding-right:8px;border-right:1px solid white;">{{ section.settings.term-text }}</a>
                  <a href="" class="privacy-ancher" style="padding-left:8px;">{{ section.settings.privacy-text}}</a>
               </p>
               <br>
               <p class="footer-discription">{{ section.settings.footer-para-text }}
               </p>
            </div>
            <div class="Contact-data">
               <p class="contact-text" style="color:{{section.settings.contact-text-color}};font-size:{{ section.settings.contact-font}}px;">{{ section.settings.contact-text }}</p>
               <p class="number-ancher" style="font-size:{{section.settings.number-ancher-font}}px;"><a href="" style="color:{{section.settings.number-ancher-color}};">{{ section.settings.number-eml-text}}</a></p>
               <p class="neuro-md-email" style="color:{{section.settings.eml-color}};font-size:{{section.settings.eml-font}}px;">{{ section.settings.email-text }}</p>
               <p class="timing" style="color:{{section.settings.timing-colors}};font-size:{{ section.settings.timing-font}}px;">{{ section.settings.timing-text}}</p>
               <p class="timing" style="color:{{section.settings.location-colors}};font-size:{{ section.settings.location-font}}px;">{{ section.settings.location-text }}</p>
            </div>
            <div class="footer-img">
               <div class="footer-image">
                  {% if section.settings.footer-img-two  != blank %}
              <img src="{{ section.settings.footer-img-two | img_url: 'large' }}" alt="{{ section.settings.footer-img-two}}">
            {% endif %}
               </div>
            </div>
            <div class="media-rate-para" style="font-size:{{ section.settings.rate-para-font}}px;color:{{section.settings.rate-colors}};">
               {{ section.settings.media-rate-text }}
            </div>
         </div>
      </div>
   </div>



{% schema %}
