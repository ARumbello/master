.news-image {
    display: none;
}
@media screen and (min-width: 100px) and (max-width: 640px){ 
.news-image {
    display: block!important;
}
.newsslider .slick-next {
    right: 5px;
    top: 55%;
} 
.proofs .slick-next {
    right: 5px;
    top: 51.2%;
}  
