</style>
<script type="text/javascript">
  $(document).ready(function(){
    $('.newsslider').slick({
      infinite: true,   
      slidesToShow: 1,
      slidesToScroll: 1,
      dots: true,
      arrows: true,
      responsive: [
        {
          breakpoint: 1024,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1
          }
        },
        {
          breakpoint: 768,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1
          }
        },
        {
          breakpoint: 640,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1
          }
        }
      ]
    });
     
  });
</script>
{% schema %}
  {
    "name": "news slider",
	"class": "prod-video-reviews",
	"max_blocks" : 20,
    "settings": [ 
        {
    		"type": "header",
            "content": "Heading"
		},
          {
            "type" : "image_picker",
            "id" : "logo1",
            "label" : "News Logo"
          },
          {
            "type" : "image_picker",
            "id" : "logo2",
            "label" : "News Logo"
          }
    ],
    "blocks": [
      {
        "type" : "text",
    	"name" : "Block",
        "settings":[ 
          {
            "type" : "text",
            "id" : "newstitle",
            "label" : "News quotes"
          },
          {
            "type" : "image_picker",
            "id" : "newsimage",
            "label" : "News Logo"
          }
        ]
      }
    ]
  }
{% endschema %}