 
<div class="news-section">
    <div class="container">
      <div class="row"> 
        <div class="col-12">
          <div class="newsslider new-dots with-arrow">
            {% for block in section.blocks %}
              <div class="news_slider_wrapper"> 
                <div class="news_content"> 
                  <h4>{{ block.settings.newstitle }}</h4>
                  <div class="news-image">
                  {% if block.settings.newsimage != blank %}
                  {%- assign img_url = block.settings.newsimage | img_url: 'large', crop: 'center' -%}
                  <img data-srcset="https:{{ img_url }}" alt="{{ block.settings.newsimage.alt | default: shop.name }}" {% include 'lazy_load' %}>
                  {% endif %}  </div>
                </div>
              </div>
            {% endfor %}
          </div>
          
          
        <div class="col-12">
        	<div class="feature_news_logos">
            	<div class="newslogo"><img class="myclass1" alt="{{ section.settings.logo1.alt}}" src="{{ section.settings.logo1 | img_url: 'large' }}" /></div>
            	<div class="newslogo"><img class="myclass1" alt="{{ section.settings.logo2.alt}}" src="{{ section.settings.logo2 | img_url: 'large' }}" /></div>
            </div>
        </div>
        </div>
      </div>
    </div>
</div>

<style>
.news-section{
    padding: 50px 200px; 
}
.newsslider {
    background: #E7F5FF;
    border-radius: 8px;
    padding: 40px;
    text-align: center;
}

.news_content h4 {
    color: #012940;
    text-align: center;
    font-family: Poppins;
    font-size: 18px;
    font-style: italic;
    font-weight: 400;
    line-height: 140%;
    padding-bottom:15px;
}
.news-image {
    margin-top: 20px;
}
.news-image img {
    width: 160px;
    margin: 0 auto;
} 
 
.feature_news_logos {
    margin: 0 auto;
    width: 300px;
    display: flex;
    justify-content: space-evenly;
}
