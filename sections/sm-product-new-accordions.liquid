            <script>
document.querySelectorAll(".headline-acc").forEach((el) => {
            el.addEventListener("click", (ev) => {
                if (el.querySelector("p").innerText === "Reviews") {
                    el.classList.toggle("border");
                }
                if (ev.target.classList.contains("headline-acc")) {
                    ev.target.parentNode
                        .querySelector(".hidden-acc")
                        .classList.toggle("hide");
                    ev.target.parentNode
                        .querySelector(".acc-arrow-open")
                        .classList.toggle("hide");
                    ev.target.parentNode
                        .querySelector(".acc-arrow-close")
                        .classList.toggle("hide");
                }
                if (
                    ev.target.classList.contains("acc-q") ||
                    ev.target.classList.contains("arrow-wrapper")
                ) {
                    ev.target.parentNode.parentNode
                        .querySelector(".hidden-acc")
                        .classList.toggle("hide");
                    ev.target.parentNode.parentNode
                        .querySelector(".acc-arrow-open")
                        .classList.toggle("hide");
                    ev.target.parentNode.parentNode
                        .querySelector(".acc-arrow-close")
                        .classList.toggle("hide");
                }
                if (
                    ev.target.classList.contains("acc-arrow-close") ||
                    ev.target.classList.contains("acc-arrow-open")
                ) {
                    ev.target.parentNode.parentNode.parentNode
                        .querySelector(".hidden-acc")
                        .classList.toggle("hide");
                    ev.target.parentNode.parentNode.parentNode
                        .querySelector(".acc-arrow-open")
                        .classList.toggle("hide");
                    ev.target.parentNode.parentNode.parentNode
                        .querySelector(".acc-arrow-close")
                        .classList.toggle("hide");
                }
            });
        }); 
              
            </script>
 